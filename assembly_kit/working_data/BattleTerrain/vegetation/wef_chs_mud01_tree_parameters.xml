<trees>
	<group group_name="decal_stone_wef_chsmudblend01">
		<texture>mud01</texture>					<!-- texture channel that must exist in order to pick group (well, actually the value must be above 0.05) --> 
		<texture_for_probability>mud01</texture_for_probability>	<!-- texture channel to use for probability, if not defined texture is used -->

		<group_channel_range_low>0.5</group_channel_range_low>		<!-- bottom of blend map value range -->
		<group_channel_range_high>1.0</group_channel_range_high>		<!-- top of blend map value range -->
		<group_channel_range_falloff>0.3</group_channel_range_falloff>	<!-- range within the defined range that is always the top probability, in this case the whole range -->

		<height_range_low>-5000</height_range_low>			<!-- bottom of height map range -->
		<height_range_high>5000</height_range_high>			<!-- top of height map range -->
		<height_range_falloff>10000</height_range_falloff>			<!-- range within the defined range that is always the top probability, in this case only the very centre -->
		
		<probability>0.05</probability>					<!-- the base probability which is adjusted based upon the above -->
		
		<grouping_probability>0</grouping_probability>		<!-- the probability of swapping out near objects with objects from the same group -->
		<grouping_radius>0</grouping_radius>				<!-- the radius within which objects may be swapped out -->
		<decal>
			<model>generic/brt_stones_01</model>
			<parallax>0.2</parallax>
			<base_scale_x>1</base_scale_x>
			<base_scale_y>1</base_scale_y>
			<base_scale_z>1</base_scale_z>
			<decal_flags apply_to_terrain="true" apply_to_objects="false" render_above_snow="false" blend_normals="false" />
			
			<size_low>0.55</size_low>
			<size_high>5</size_high>
			<blendmap_range_low>0.3</blendmap_range_low>
			<blendmap_range_high>1</blendmap_range_high>
			<blendmap_range_falloff>0.0</blendmap_range_falloff>
			<height_low>-5000</height_low>
			<height_high>5000</height_high>
			<height_falloff>0</height_falloff>
			<humidities>
				<humidity>arid</humidity>
				<humidity>humid</humidity>
			</humidities>
			<categories>
				<category>wef_chs</category>
				
			</categories>
			<probability>0.1</probability>
			<minimum_seperation>1</minimum_seperation>
			<enable_debug_lines>false</enable_debug_lines>
			<debug_lines_red>1.0</debug_lines_red>
			<debug_lines_green>0.65</debug_lines_green>
			<debug_lines_blue>0.90</debug_lines_blue>
		</decal>
